#include <stdio.h>

int main() {
    int n, i;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    if (n < 2) {
        printf("Array should have at least two elements.\n");
        return 1;
    }

    int arr[n];
    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int first, second;
    first = second = arr[0];

    
    for (i = 1; i < n; i++) {
        if (arr[i] > first) {
            first = arr[i];
        }
    }

    
    int found = 0;
    for (i = 0; i < n; i++) {
        if (arr[i] != first) {
            if (!found || arr[i] > second) {
                second = arr[i];
                found = 1;
            }
        }
    }

    if (found) {
        printf("The second largest element is: %d\n", second);
    } else {
        printf("There is no second largest element (all elements may be equal).\n");
    }

    return 0;
}
